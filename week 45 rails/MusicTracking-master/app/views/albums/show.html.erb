<h1> This is <%= @album.name %>'s page! </h1>
<h2> Here are the tracks in <%= @album.name %> </h2>
<% @album.tracks.each do |track| %>
  <%= link_to track.name, track_url(track) %>
  <%= button_to "Delete track", track_url(track), method: :delete %>
  <br>
<% end %>

<%= link_to "Create a track", new_album_track_url(@album.id) %>

<%= link_to "Edit this album!", edit_album_url(@album) %>

<%= link_to "Back to #{@album.band.name}", band_url(@album.band.id) %>
